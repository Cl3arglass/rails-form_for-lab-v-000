<h3>School Class Form</h3>

<%= form_tag students_path do %>
  <label>Title:</label><br>
  <%= text_field_tag "school_class_title", :title %><br>

  <label>Room Number:</label><br>
  <%= number_field_tag "school_class_room_number", :room_number %><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag "Create School class" %>
<% end %>
